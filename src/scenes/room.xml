        </primitive>

        <primitive id="cubeFace">
            <rectangle x1="-0.5" y1="-0.5" x2="0.5" y2="0.5" />
        </primitive>

        <primitive id="cylinder">
            <cylinder base="0.5" top="0.5" height="1" slices="20" stacks="20" />
        </primitive>

        <primitive id="sphere">
            <sphere radius="0.5" slices="50" stacks="20" />
        </primitive>

        <primitive id="triangle">
            <triangle x1="0" y1="1" z1="0" x2="0" y2="0" z2="0" x3="1" y3="0" z3="0" />
        </primitive>  

        <primitive id="triangle2">
