    <transformations>
        <transformation id="transformationWall">
            <translate x="20" y="9" z="0.25" />
            <scale x="40" y="18" z="0.5" />
        </transformation>
        <transformation id="rotation">
            <rotate axis="x" angle="90" />
        </transformation>
        <transformation id="transformationFloor">
            <translate x="0" y="0.1" z="40" />
            <rotate axis="x" angle="-90" />
            <translate x="20" y="20" z="0" />
            <scale x="40" y="40" z="1" />
        </transformation>
        <transformation id="translationTimer">
            <translate x="4" y="0.3" z="0.2" />
        </transformation>
        
    </transformations>
    
    <animations>
        <animation id="flag_move">
            <keyframe instant="24">
                <translate x="0" y="0" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0" />
                <scale x="1" y="1" z="1" />
            </keyframe>
            <keyframe instant="30">
                <translate x="0" y="3" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0" />
                <scale x="1" y="1" z="1" />
            </keyframe>
        </animation>
    </animations>


