<lxs>
    <scene root="rootComp" axis_length="5" />

    <views default="player_camera" >
        <perspective id="player_camera" near="0.1" far="90" angle="60">
            <from x="10" y="7" z="25" />
            <to x="10" y="5" z="20" />
        </perspective>
        <perspective id="spectator_camera" near="0.1" far="90" angle="60">
            <from x="13" y="7.5" z="20" />
            <to x="10" y="5" z="20" />
